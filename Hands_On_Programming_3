        String MyString = "543";
        
        public void RotateArray(string str)
        {

            //string str = "543";
            string OutPut = "";
            for (int i = str.Length - 1; i >= 0; i--)
            {
                OutPut += str.Substring(i, 1);
            }

            //textBox1.Text = OutPut;

        }
